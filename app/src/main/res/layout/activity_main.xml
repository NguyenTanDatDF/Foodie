<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.MainActivity"
    >


    <FrameLayout
        android:id="@+id/frame_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:layout_above="@+id/meow_bot_nav"
        android:layout_marginBottom="-25dp" />

    <com.etebarian.meowbottomnavigation.MeowBottomNavigation

        android:id="@+id/meow_bot_nav"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:animationCache="false"
        android:soundEffectsEnabled="false"
        app:mbn_backgroundBottomColor="#00B0F0"
        app:mbn_circleColor="#C51B1B"
        app:mbn_countBackgroundColor="#ff6f00"
        app:mbn_countTextColor="#6720AE"
        app:mbn_defaultIconColor="#000000"
        app:mbn_rippleColor="#A12323"
        app:mbn_selectedIconColor="#000000"
        app:mbn_shadowColor="#002196F3" />


</RelativeLayout>